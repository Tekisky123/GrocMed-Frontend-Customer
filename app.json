{
  "expo": {
    "name": "GrocMed",
    "slug": "ReactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "reactnative",
    "userInterfaceStyle": "automatic",

    "entryPoint": "./node_modules/expo-router/entry",

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.tekisky.ReactNative",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo.png"
      }
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "extra": {
      "eas": {
        "projectId": "7a31d9b9-c599-43cd-8a2d-0a88b851171c"
      }
    }
  }
}
